{
	"name": "ppm",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"test:ui": "vitest --ui",
		"test:run": "vitest --run",
		"test:db": "TEST_DB_CLEANUP=true vitest",
		"test:timing": "vitest --grep='timing|timeout|duration'",
		"test:concurrent": "vitest --grep='concurrent|race|parallel'",
		"test:integration": "vitest --grep='integration'",
		"test:unit": "vitest --grep='unit'",
		"gentypes": "npx supabase gen types typescript --local --schema public > src/lib/shared/database.types.ts"
	},
	"devDependencies": {
		"@effect/language-service": "^0.44.0",
		"@effect/platform": "^0.92.1",
		"@effect/platform-node": "^0.98.3",
		"@effect/vitest": "^0.26.0",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.37.0",
		"@lucide/svelte": "^0.545.0",
		"@skeletonlabs/skeleton": "^3.2.2",
		"@skeletonlabs/skeleton-svelte": "^1.5.3",
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/kit": "^2.46.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.14",
		"@vitest/browser": "^3.2.4",
		"@vitest/coverage-v8": "^3.2.4",
		"clsx": "^2.1.1",
		"eslint": "^9.37.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"playwright": "^1.56.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.39.11",
		"svelte-check": "^4.3.3",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.14",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.0",
		"vite": "^7.1.9",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.1.0"
	},
	"dependencies": {
		"@internationalized/date": "^3.10.0",
		"@supabase/ssr": "^0.7.0",
		"@supabase/supabase-js": "^2.75.0",
		"bits-ui": "^2.11.5",
		"effect": "^3.18.4",
		"http-status-codes": "^2.3.0",
		"lucide-svelte": "^0.545.0"
	}
}
