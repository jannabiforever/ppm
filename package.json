{
	"name": "ppm",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"test:ui": "vitest --ui",
		"test:run": "vitest --run",
		"test:db": "TEST_DB_CLEANUP=true vitest",
		"test:timing": "vitest --grep='timing|timeout|duration'",
		"test:concurrent": "vitest --grep='concurrent|race|parallel'",
		"test:integration": "vitest --grep='integration'",
		"test:unit": "vitest --grep='unit'",
		"gentypes": "npx supabase gen types typescript --local --schema public > src/lib/shared/database.types.ts"
	},
	"devDependencies": {
		"@effect/language-service": "^0.36.0",
		"@effect/platform": "^0.90.2",
		"@effect/platform-node": "^0.95.0",
		"@effect/vitest": "^0.25.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@skeletonlabs/skeleton": "^3.1.7",
		"@skeletonlabs/skeleton-svelte": "^1.3.1",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@vitest/browser": "^3.2.3",
		"@vitest/coverage-v8": "^3.2.4",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"@internationalized/date": "^3.9.0",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.53.0",
		"bits-ui": "^2.9.2",
		"effect": "^3.17.6",
		"http-status-codes": "^2.3.0",
		"lucide-svelte": "^0.539.0"
	}
}
